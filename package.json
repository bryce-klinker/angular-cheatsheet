{
  "name": "angular-cheatsheet",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "all:start": "concurrently \"yarn server:start\" \"yarn client:start\"",
    "all:stop": "concurrently \"yarn server:stop\" \"yarn client:stop\"",
    "all:wait": "concurrently \"yarn server:wait\" \"yarn client:wait\"",
    "client:start": "yarn ng serve",
    "client:stop": "kill -9 $(lsof -t -i:4200 -sTCP:LISTEN) || true",
    "client:wait": "wait-on tcp:4200",
    "server:start": "json-server db.json",
    "server:stop": "kill -9 $(lsof -t -i:3000 -sTCP:LISTEN) || true",
    "server:wait": "wait-on tcp:3000",
    "ng": "ng",
    "start": "concurrently \"yarn client:start\" \"yarn server\" ",
    "prebuild": "yarn clean",
    "build": "ng build",
    "prebuild:prod": "yarn clean",
    "build:prod": "ng build --configuration=\"production\"",
    "test": "ng test",
    "lint": "ng lint",
    "cypress:run": "yarn cypress run",
    "cypress:open": "yarn cypress open",
    "e2e:open": "concurrently \"yarn all:start\" \"yarn all:wait && yarn cypress:open && yarn all:stop || (yarn all:stop && false)\"",
    "e2e": "concurrently \"yarn all:start\" \"yarn all:wait && yarn cypress:run && yarn all:stop || (yarn all:stop && false)\""
  },
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@cypress/webpack-preprocessor": "^4.1.2",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@testing-library/angular": "8.2.0",
    "@types/cypress-cucumber-preprocessor": "1.14.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concurrently": "5.1.0",
    "cypress": "4.0.2",
    "cypress-cucumber-preprocessor": "2.0.1",
    "jasmine-core": "~3.4.0",
    "json-server": "0.15.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "0.0.32",
    "protractor": "~5.4.0",
    "rimraf": "3.0.2",
    "ts-loader": "6.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "wait-on": "4.0.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
